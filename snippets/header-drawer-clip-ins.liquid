        {% render 'icon-arrow' %}
        BACK
      </button>
      <ul
        class="menu-drawer__menu list-menu"
        role="list"
        tabindex="-1"
      >
        <li>
          {% assign object = collections['seamless-clip-in-hair-extensions-double-drawn-hair'] %}
